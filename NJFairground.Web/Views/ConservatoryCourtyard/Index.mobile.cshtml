@using NJFairground.Web.Utilities
@using NJFairground.Web.Models
@model PageModel
@{
    ViewBag.Title = string.Format("{0} | Sussex County Fairgrounds", Model.PageDesc);
    ViewBag.PageId = Model.PageName;
}
<h2 class="title">@Model.PageDesc</h2>
<div class="ui-grid-solo highlights courtyard inner-page">
    <img class="inner-banner" src='@Model.PageImageUrl' alt="">
    <div class="ui-block-a highlight">
        <div class="complextext">
            @Html.Raw(Model.PageContent)
        </div>
    </div>
    @foreach (var pageItemdata in Model.PageItems)
    {
        <div class="ui-block-a highlight">
            <div class="complextext">
                <h4>@pageItemdata.PageHeaderText</h4>
                <p>@pageItemdata.PageSubHeaderText</p>
            </div>
        </div>
    }
</div>
