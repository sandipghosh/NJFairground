<!DOCTYPE html>
<html>
<head>

    <title>@ViewBag.Title</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link type="text/css" rel="stylesheet" href='@Url.Content("~/Styles/jquery.mobile-1.4.2.min.css")' />
    <link type="text/css" rel="stylesheet" href='@Url.Content("~/Styles/style.css")' />

    @*@Styles.Render("~/Styles/CommonStyle")*@
    @*@Scripts.Render("~/Scripts/CommonScript")*@

    <!--- JQuery Mobile 1.4.2 --->

    <script src='@Url.Content("~/Scripts/jquery-2.1.1.min.js")'></script>
    <script src='@Url.Content("~/Scripts/jquery-migrate-1.2.1.min.js")'></script>
    <script src='@Url.Content("~/Scripts/jquery.mobile-1.4.2.min.js")'></script>

</head>
<body>
    <div data-role="page" id="@ViewBag.PageId" data-title="@ViewBag.Title">@*"Info | Sussex County Fairgrounds">*@
        @Html.Partial("_Header")
        <!-- /header -->

        <div role="main" class="ui-content">
            @RenderSection("navbar", required: false)
            <div class="ui-grid-solo inner-page">
                @RenderSection("innerTitle", required: false)
            </div>
            @RenderBody()


            <span class="presented-by">
                <img src="~/Styles/images/presentedby.png" alt=""></span>
        </div>
        <!-- /content -->

        @Html.Partial("_Footer")
    </div>
    <!-- /page -->

</body>
</html>
