@model List<NJFairground.Web.Models.PageItemModel>
@{
    ViewBag.Title = "Shopping | Sussex County Fairgrounds";
    ViewBag.PageId = "shopping";
}

<h2 class="title">Shopping</h2>
<div class="ui-grid-solo highlights complex inner-page">
    <img class="inner-banner" src='@Url.Content("~/Styles/images/Fairgrounds 1.jpg")' alt="">
    <div class="ui-block-a highlight">
        <div class="complextext">

            <h4>SHOPPING:</h4>
            <br />
            <p>
                Come and shop in our Craft Tent featuring candles, ceramics, pottery, 
                stained glass, tie die and sauces to name some.  Walk through the Commercial Tent, 
                and visit the individual vendors scattered throughout the grounds!
            </p>
        </div>
    </div>
</div>
